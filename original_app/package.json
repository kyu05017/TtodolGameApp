{
  "name": "TtodolGameApp",
  "version": "0.1.0",
  "private": true,
  "sideEffects": [
    "*.css",
    "*.scss",
    "src/web/*.js"
  ],
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "webpack serve --config webpack.config.js --mode development --open",
    "web-dev": "webpack serve --config webpack.config.js --mode development --port 3001",
    "build-web": "webpack --config webpack.config.js --mode production",
    "build-android": "eas build --platform android",
    "build-android-optimized": "node scripts/build-android-optimized.js",
    "build-android-optimized-dev": "node scripts/build-android-optimized.js --dev --verbose",
    "build-ios": "eas build --platform ios",
    "build-all": "eas build --platform all",
    "bundle-android": "npx expo export:embed --platform android --entry-file index.js --bundle-output dist-android/index.android.bundle --assets-dest dist-android --dev false --minify true --reset-cache",
    "bundle-android-dev": "npx expo export:embed --platform android --entry-file index.js --bundle-output dist-android/index.android.bundle --assets-dest dist-android --dev true --minify false --reset-cache",
    "post-process-bundle": "node scripts/post-bundle-processor.js dist-android/index.android.bundle --verbose",
    "submit-android": "eas submit --platform android",
    "submit-ios": "eas submit --platform ios",
    "prebuild": "expo prebuild",
    "test": "jest",
    "lint": "eslint .",
    "lint-fix": "eslint . --fix",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf node_modules && npm install",
    "reset-cache": "expo start --clear"
  },
  "dependencies": {
    "@expo/metro-runtime": "~5.0.4",
    "@expo/vector-icons": "^14.0.4",
    "@react-native-async-storage/async-storage": "2.1.2",
    "expo": "~53.0.0",
    "expo-av": "~15.1.0",
    "expo-constants": "~17.1.0",
    "expo-device": "~7.1.0",
    "expo-keep-awake": "~14.1.4",
    "expo-linear-gradient": "^14.1.5",
    "expo-screen-orientation": "~8.1.0",
    "expo-splash-screen": "~0.30.0",
    "expo-status-bar": "~2.2.3",
    "matter-js": "^0.19.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.5",
    "react-native-game-engine": "^1.2.0",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-reanimated": "~3.17.4",
    "react-native-responsive-dimensions": "^3.1.1",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-svg": "15.11.2"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/eslint-parser": "^7.28.0",
    "@babel/plugin-proposal-export-default-from": "^7.27.1",
    "@babel/plugin-proposal-export-namespace-from": "^7.18.9",
    "@babel/plugin-transform-class-properties": "^7.27.1",
    "@babel/plugin-transform-export-namespace-from": "^7.27.1",
    "@babel/plugin-transform-flow-strip-types": "^7.27.1",
    "@babel/plugin-transform-modules-commonjs": "^7.27.1",
    "@babel/plugin-transform-private-methods": "^7.27.1",
    "@babel/plugin-transform-private-property-in-object": "^7.27.1",
    "@babel/plugin-transform-typescript": "^7.28.0",
    "@babel/preset-env": "^7.28.0",
    "@babel/preset-flow": "^7.27.1",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@react-native-community/cli": "^19.1.1",
    "@react-native/metro-config": "^0.80.2",
    "@types/jest": "^29.5.13",
    "@types/node": "^24.1.0",
    "@types/react": "~19.0.10",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "babel-loader": "^9.2.1",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "babel-preset-expo": "~13.0.0",
    "copy-webpack-plugin": "^12.0.2",
    "eslint": "^8.57.0",
    "eslint-config-expo": "~9.2.0",
    "eslint-plugin-react": "^7.37.5",
    "html-webpack-plugin": "^5.6.3",
    "jest": "^29.4.0",
    "metro-react-native-babel-transformer": "^0.77.0",
    "prettier": "^3.3.2",
    "react-native-web": "^0.20.0",
    "ts-node": "^10.9.2",
    "typescript": "~5.8.3",
    "webpack": "^5.100.2",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2"
  },
  "engines": {
    "node": ">=20"
  }
}
