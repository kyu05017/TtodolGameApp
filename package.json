{
  "name": "TtodolGameApp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "web": "webpack serve --config webpack.config.js --mode development --open",
    "web-dev": "webpack serve --config webpack.config.js --mode development --port 3000",
    "web-prod": "webpack serve --config webpack.config.js --mode production --port 3000",
    "build-web": "webpack --config webpack.config.js --mode production",
    "start-both": "concurrently \"npm run start\" \"npm run web-dev\"",
    "test-web": "npm run web-dev",
    "test-mobile": "npm run start",
    "clean": "rm -rf node_modules dist && npm install",
    "reset-cache": "npx react-native start --reset-cache"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native/new-app-screen": "0.80.1",
    "@react-navigation/native": "^7.1.14",
    "@react-navigation/stack": "^7.4.2",
    "matter-js": "^0.20.0",
    "react": "19.1.0",
    "react-native": "0.80.1",
    "react-native-game-engine": "^1.2.0",
    "react-native-gesture-handler": "^2.27.1",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-modal": "^14.0.0-rc.1",
    "react-native-reanimated": "^3.18.0",
    "react-native-responsive-dimensions": "^3.1.1",
    "react-native-safe-area-context": "^5.5.2",
    "react-native-screens": "^4.12.0",
    "react-native-sound": "^0.12.0",
    "react-native-svg": "^15.12.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.25.3",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@babel/runtime": "^7.25.0",
    "@react-native-community/cli": "19.0.0",
    "@react-native-community/cli-platform-android": "19.0.0",
    "@react-native-community/cli-platform-ios": "19.0.0",
    "@react-native/babel-preset": "0.80.1",
    "@react-native/eslint-config": "0.80.1",
    "@react-native/metro-config": "0.80.1",
    "@react-native/typescript-config": "0.80.1",
    "@types/jest": "^29.5.13",
    "@types/react": "^19.1.0",
    "@types/react-test-renderer": "^19.1.0",
    "babel-loader": "^10.0.0",
    "eslint": "^8.19.0",
    "html-webpack-plugin": "^5.6.3",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-dom": "^19.1.0",
    "react-native-web": "^0.20.0",
    "react-test-renderer": "19.1.0",
    "typescript": "5.0.4",
    "webpack": "^5.100.2",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2",
    "concurrently": "^8.2.2",
    "copy-webpack-plugin": "^12.0.2"
  },
  "engines": {
    "node": ">=18"
  }
}
